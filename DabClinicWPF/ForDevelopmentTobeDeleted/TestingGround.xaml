<Window x:Class="DabClinicWPF.ForDevelopmentTobeDeleted.TestingGround"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DabClinicWPF.ForDevelopmentTobeDeleted"
        mc:Ignorable="d"
        Title="TestingGround" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dtgShowData"
                  HorizontalAlignment="Left"
                  Height="239"
                  Margin="80,65,0,0"
                  VerticalAlignment="Top"
                  Width="480"
                  d:ItemsSource="{d:SampleData ItemCount=20}"
                  Loaded="dtgShowData_Loaded" />

    </Grid>
</Window>
